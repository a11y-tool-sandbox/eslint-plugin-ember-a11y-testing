{
  "name": "eslint-plugin-ember-a11y-testing",
  "version": "0.0.0",
  "description": "ESLint plugin for ember-a11y-testing",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Jamie White",
  "type": "module",
  "main": "lib/index.mjs",
  "exports": {
    ".": "./lib/index.mjs",
    "./recommended": "./lib/recommended.mjs"
  },
  "scripts": {
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint --config eslint.config.mjs --fix .",
    "test": "mocha --config .mocharc.cjs tests --recursive"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@eslint/markdown": "^6.4.0",
    "common-tags": "^1.8.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^14.0.0",
    "mocha": "^10.3.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.18.0 || >=20"
  },
  "license": "ISC",
  "files": [
    "README.md",
    "lib/**/*.js",
    "lib/**/*.mjs",
    "recommended.mjs",
    "package.json"
  ],
  "packageManager": "yarn@3.2.3"
}
